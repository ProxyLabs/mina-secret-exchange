{
  "name": "esbuild-vue-cli",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "serve": "node src/server.js",
    "esbuild": "node src/build.js",
    "build": "npx tsc && npm run esbuild && npm run serve",
    "test": "npx tsc && jest --collect-coverage --verbose"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "jest": "^27.4.4",
    "snarkyjs": "^0.1.11",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "esbuild": "^0.14.2",
    "esbuild-vue": "^1.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
